@article{hornby2004functional,
  title={Functional scalability through generative representations: the evolution of table designs},
  author={Hornby, Gregory S},
  journal={Environment and Planning B: Planning and Design},
  volume={31},
  number={4},
  pages={569--587},
  year={2004},
  publisher={SAGE Publications},
  annote = {Hornby's work in this paper investigates how \emph{Generative Encodings} can be used to evolve
            novel \emph{and} functional designs in 3D models.  He specifically works with a grammatical
            representation of a table, and examines how a generative representation is able to not just
            maintain successful structures within the model, but also propagate and build on them, providing
            much more functional design evolutions over direct encodings}
}

