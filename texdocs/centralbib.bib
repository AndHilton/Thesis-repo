@article{rieffel2009automated,
  title={Automated discovery and optimization of large irregular tensegrity structures},
  author={Rieffel, John and Valero-Cuevas, Francisco and Lipson, Hod},
  journal={Computers \& Structures},
  volume={87},
  number={5},
  pages={368--379},
  year={2009},
  publisher={Elsevier},
  annote = {This paper explores the possibility of using genetic algorithms to evolve designs for tensegrities.
            Previously designing large tensegrity structures was too difficult to do by hand, except for the
            cases of regular tensegrities.  Using a graph-based encoding of the structures, where nodes 
            are numbered to represent the rigid elements and the edges represent the tensile connections, this
            paper showed that it is possible to generate an L-system grammar and evolve the structures.  It
            further shows that using an evolved method such as this allows for a greater range of valid structures.
            Finally the paper addresses some of the modifications that were needed to adjust for the actual physical
            limitations of the structures}
}

