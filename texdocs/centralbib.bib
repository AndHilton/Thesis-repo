@book{prusinkiewicz2012algorithmic,
  title={The algorithmic beauty of plants},
  author={Prusinkiewicz, Przemyslaw and Lindenmayer, Aristid},
  year={2012},
  publisher={Springer Science \& Business Media},
  annote = {This book was one of the first popular publications on the idea of using L-systems and grammar encodings
            to represent more complex structures, in this case plants.  It provides a very good introduction to the
            concepts, as well as the mathematics behind it, using interesting examples (both theoretical and real-world)
            as well as figures and diagrams to explain the processes.  This was a very good starting point for me in terms
            of transitioning from my first project idea into the current genetic algorithms / biological model driven
            research, and looking at how grammar encodings could be applied to different systems.  I have slowed down
            on reading this book because it is not as directly relevant right now, but I think that finishing it before I
            really begin the bulk of the work for my thesis will be pertinent}
}

